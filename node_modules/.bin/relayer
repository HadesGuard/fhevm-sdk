#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/abc/Personal/confidential-chat/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@zama-fhe/relayer-sdk/bin/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@zama-fhe/relayer-sdk/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@zama-fhe/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/abc/Personal/confidential-chat/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@zama-fhe/relayer-sdk/bin/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@zama-fhe/relayer-sdk/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@zama-fhe/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@zama-fhe/relayer-sdk/bin/relayer.js" "$@"
else
  exec node  "$basedir/../@zama-fhe/relayer-sdk/bin/relayer.js" "$@"
fi

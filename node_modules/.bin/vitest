#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/abc/Personal/confidential-chat/node_modules/.pnpm/vitest@2.1.9_@edge-runtime+vm@3.2.0_@types+node@18.19.127_jsdom@27.0.0_bufferutil@4.0.9_a4199ce87bde483eb02a28af99120fc2/node_modules/vitest/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/vitest@2.1.9_@edge-runtime+vm@3.2.0_@types+node@18.19.127_jsdom@27.0.0_bufferutil@4.0.9_a4199ce87bde483eb02a28af99120fc2/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/abc/Personal/confidential-chat/node_modules/.pnpm/vitest@2.1.9_@edge-runtime+vm@3.2.0_@types+node@18.19.127_jsdom@27.0.0_bufferutil@4.0.9_a4199ce87bde483eb02a28af99120fc2/node_modules/vitest/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/vitest@2.1.9_@edge-runtime+vm@3.2.0_@types+node@18.19.127_jsdom@27.0.0_bufferutil@4.0.9_a4199ce87bde483eb02a28af99120fc2/node_modules:/Users/abc/Personal/confidential-chat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
